#include <iostream>
using namespace std;

int main(){
	int num[5][3], x, y;
	cout << "Enter Integers in a 5 x 3 Array Matrix: \n";
	for (x = 0; x <= 4; x++)
		for (y = 0; y <= 2; y++){
			cout << "num[" << x << "][" << y << "] = ";
			cin >> num[x][y];
		}
	cout << "\nDisplay Integers in 5 x 3 Matrix: \n";
	for (x = 0; x <= 4; x++){
		for (y = 0; y <= 2; y++){
			cout << "num[" << x << "][" << y << "] = " << num[x][y] << "\t";
		}
		cout <<"\n";
	}
	return 0;	
}
